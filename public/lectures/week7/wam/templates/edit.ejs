<li class="container-fluid">
    <ul class="list-group">
        <li class="list-group-item active">
            <%= entityName %>
        </li>
    </ul>
<%= entityName%> Edit Page

    <% for(var fieldName in fields) {
        var field = fields[fieldName];
        var placeholder = field.placeholder;
    %>
<div>
    <label for="<%= fieldName%>"><%= field.label ? field.label : fieldName%></label>
        <% if (!field.type || field.type === "text") {

        %>
    <input id="<%= fieldName%>"
           ng-model=""
           placeholder="<%= placeholder%>"
           class="form-control"/>
        <%} else if (field.type === "date") {
        %>
    <input id="<%= fieldName %>"
           ng-model=""
           placeholder=""
           class="" />

</div>
<% }
 %>
    <button ng-click="" ng-if="model.<%=entityName%>Id === 'new'" class="btn btn-primary btn-block">Create</button>
    <button ng-click="" ng-if="model.<%=entityName%>Id !== 'new'" class="btn btn-success btn-block">Update</button>
        </li>
    </ul>
</div>
