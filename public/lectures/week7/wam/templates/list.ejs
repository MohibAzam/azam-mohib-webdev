<br/>
<div class="container-fluid">
    <ul class="list-group">
        <li class="list-group-item active">
            <a href="#!/<%= entityName %>/new" class="pull-right" style="color: #fff;">
                <span class="glyphicon glyphicon-plus"></span>
            </a>
            <%= entityName%>
        </li>
        <li ng-repeat="<%= entityName%>" in model.<%= entityName%>">
            <div class="row">
                <%  for(var fieldName in fields) {
                        var field = fields[fieldName];
                %>
                <div class="col-xs<%= field.cols %>">
                    {{<%= entityName %>.<%= fieldName %>
                    <%= field.type && field.type === "date" ? "| date:short" : "format"%>
                </div>
                }

                %>
                <div class="col-xs-1">
                    <a class="pull-right"
                </div>
            </div>
            {{item}}
        </li>
    </ul>
    {{model.<%= entityName%>s}}
</div>